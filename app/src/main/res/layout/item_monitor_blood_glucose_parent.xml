<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    >
    <data>
        <variable
            name="monitorbloodGlucose"
            type="com.s.diabetesfeeding.data.db.entities.MonitorbloodGlucoseX" />
        <import type="android.view.View"/>
    </data>
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    >
    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/cat_item_recycler"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        />
<View
    android:id="@+id/view_rv_divider"
    android:layout_width="match_parent"
    android:layout_height="1dp"
    android:layout_marginBottom="4dp"
    android:layout_marginTop="4dp"
    android:background="@color/color_recycle_divider_parent"
    />

</LinearLayout>
</layout>